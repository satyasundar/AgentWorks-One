   "source": [
    "from PyPDF2 import PdfMerger\n",
    "\n",
    "pdfs = [\"../data/one.pdf\",\n",
    "        \"../data/two.pdf\"]\n",
    "\n",
    "merger = PdfMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(pdf)\n",
    "\n",
    "merger.write(\"../data/combined.pdf\")\n",
    "merger.close()"
   ]
